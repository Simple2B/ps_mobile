{
  "name": "ps_mobile",
  "main": "expo-router/entry",
  "version": "1.0.2",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "and-build-dev": "eas build -p android --output=build-and-dev-latest.apk --profile=development --local --non-interactive",
    "ios-build-dev": "eas build -p ios --output=builds/build-ios-dev-latest.tar.gz --profile=development --local --non-interactive",
    "storybook-generate": "sb-rn-get-stories",
    "sb": "STORYBOOK_ENABLED='true' expo start",
    "get-openapi-json": "./scripts/generate_openapi_json.sh",
    "gen-api": "orval generate --config ./orval.config.ts",
    "eas-build-post-install": "yarn orval --config ./orval.config.ts -p base",
    "type-check": "tsc --noEmit",
    "bump-patch": "npm version patch; git push --follow-tags",
    "bump-minor": "npm version minor; git push --follow-tags",
    "bump-major": "npm version major; git push --follow-tags"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@hookform/resolvers": "^3.6.0",
