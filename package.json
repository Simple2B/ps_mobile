    "@storybook/addon-ondevice-controls": "^7.6.20",
    "@storybook/react-native": "^7.6.20",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "babel-loader": "^8.3.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.1",
    "orval": "^6.30.2",
